  // 8. Nutritional Status
  static calculateNutritionalStatus(patientData: PatientData): HealthDomainResult {
    const { biomarkers, demographics, lifestyle } = patientData
    let score = 50
    const markersAvailable: string[] = []
    const markersMissing: string[] = ['Vitamin D', 'B12', 'Folate', 'Iron Studies', 'Micronutrient Panel', 'Omega-3 Index']
    const recommendations: string[] = []

    // Diet quality (primary nutrition indicator)
    if (lifestyle.diet_quality !== undefined) {
      markersAvailable.push('Diet Quality Assessment')
      if (lifestyle.diet_quality >= 4) score += 25 // Excellent diet quality
      else if (lifestyle.diet_quality === 3) score += 15 // Good diet quality
      else if (lifestyle.diet_quality === 2) score -= 5 // Poor diet quality
      else score -= 25 // Very poor diet quality
    } else {
      markersMissing.push('Diet Quality Assessment')
    }

    // Albumin (protein status and overall nutrition)
    if (biomarkers.albumin !== undefined) {
      markersAvailable.push('Protein Status (Albumin)')
      if (biomarkers.albumin >= 4.0) score += 15 // Excellent protein status
      else if (biomarkers.albumin >= 3.5) score += 8 // Good protein status
      else score -= 20 // Poor protein status
    } else {
      markersMissing.push('Albumin')
    }

    // Hemoglobin (iron status and nutrition)
    if (biomarkers.hemoglobin !== undefined) {
      markersAvailable.push('Iron Status (Hemoglobin)')
      const isWoman = demographics.gender === 'female'
      const optimalHgb = isWoman ? 13.0 : 14.5
      const lowHgb = isWoman ? 11.5 : 12.5
      
      if (biomarkers.hemoglobin >= optimalHgb) score += 15 // Excellent iron status
      else if (biomarkers.hemoglobin >= lowHgb) score += 8 // Adequate iron status
      else score -= 20 // Iron deficiency
    }

    // BMI (nutritional balance indicator)
    if (demographics.bmi !== undefined) {
      markersAvailable.push('Nutritional Balance (BMI)')
      if (demographics.bmi >= 18.5 && demographics.bmi <= 24.9) score += 12 // Optimal nutritional balance
      else if (demographics.bmi >= 25 && demographics.bmi <= 29.9) score -= 8 // Overnutrition
      else if (demographics.bmi >= 30) score -= 20 // Significant overnutrition
      else score -= 15 // Undernutrition
    }

    // Glucose control (nutritional metabolic impact)
    if (biomarkers.glucose !== undefined && biomarkers.hba1c !== undefined) {
      markersAvailable.push('Metabolic Nutrition Impact')
      if (biomarkers.glucose <= 90 && biomarkers.hba1c <= 5.4) score += 12 // Excellent nutritional metabolic impact
      else if (biomarkers.glucose >= 110 || biomarkers.hba1c >= 6.0) score -= 15 // Poor nutritional choices affecting metabolism
    }

    // Lipid profile (dietary fat quality)
    if (biomarkers.hdl_cholesterol !== undefined && biomarkers.triglycerides !== undefined) {
      markersAvailable.push('Dietary Fat Quality')
      const tgHdlRatio = biomarkers.triglycerides / biomarkers.hdl_cholesterol
      if (tgHdlRatio <= 2.0) score += 12 // Good dietary fat choices
      else if (tgHdlRatio >= 4.0) score -= 15 // Poor dietary fat choices
    }

    // Inflammatory markers (anti-inflammatory nutrition)
    if (biomarkers.c_reactive_protein !== undefined) {
      markersAvailable.push('Anti-Inflammatory Nutrition')
      if (biomarkers.c_reactive_protein <= 1.0) score += 10 // Anti-inflammatory diet
      else if (biomarkers.c_reactive_protein >= 3.0) score -= 12 // Pro-inflammatory diet
    }

    // Age-related nutritional needs
    if (demographics.age !== undefined) {
      markersAvailable.push('Age-Adjusted Nutritional Needs')
      if (demographics.age >= 65) {
        score -= 5 // Increased nutritional needs with age
        if (score >= 70) score += 5 // Bonus for meeting increased needs
      }
    }

    // Alcohol consumption (nutritional impact)
    if (lifestyle.alcohol_consumption !== undefined) {
      markersAvailable.push('Alcohol Impact on Nutrition')
      if (lifestyle.alcohol_consumption <= 3) score += 5 // Minimal nutritional impact
      else if (lifestyle.alcohol_consumption >= 7) score -= 15 // Significant nutritional impact
    }

    const finalScore = Math.max(0, Math.min(100, score))
    
    let optimizationLevel: 'poor' | 'fair' | 'good' | 'excellent' | 'optimal'
    if (finalScore >= 85) optimizationLevel = 'optimal'
    else if (finalScore >= 75) optimizationLevel = 'excellent'
    else if (finalScore >= 65) optimizationLevel = 'good'
    else if (finalScore >= 50) optimizationLevel = 'fair'
    else optimizationLevel = 'poor'

    // Generate recommendations
    if (lifestyle.diet_quality && lifestyle.diet_quality < 3) {
      recommendations.push('Focus on whole foods nutrition - vegetables, fruits, lean proteins, and healthy fats')
    }
    if (biomarkers.albumin && biomarkers.albumin < 3.8) {
      recommendations.push('Ensure adequate protein intake and consider protein quality optimization')
    }
    if (biomarkers.hemoglobin && biomarkers.hemoglobin < (demographics.gender === 'female' ? 12.0 : 13.5)) {
      recommendations.push('Address potential iron deficiency with iron-rich foods or supplementation')
    }
    if (demographics.bmi && demographics.bmi > 25) {
      recommendations.push('Consider portion control and nutrient-dense food choices for weight optimization')
    }
    if (recommendations.length === 0) {
      recommendations.push('Maintain excellent nutritional practices and consider micronutrient optimization')
    }

    const confidence = markersAvailable.length >= 4 ? 'medium' : 'low' // Cannot be high without comprehensive micronutrient testing

    return {
      domain_name: 'Nutritional Status',
      score_percentage: finalScore,
      confidence_level: confidence,
      markers_available: markersAvailable,
      markers_missing: markersMissing,
      optimization_level: optimizationLevel,
      recommendations: recommendations,
      description: `Nutritional status score of ${finalScore}% based on diet quality, nutritional biomarkers, metabolic indicators, and age-adjusted nutritional adequacy.`,
      algorithm_used: 'Comprehensive nutritional status assessment',
      reference: 'Academy of Nutrition and Dietetics Evidence-Based Guidelines 2020; WHO Nutritional Indicators 2021'
    }
  }
