    if (finalScore >= 85) optimizationLevel = 'optimal'
    else if (finalScore >= 75) optimizationLevel = 'excellent'
    else if (finalScore >= 65) optimizationLevel = 'good'
    else if (finalScore >= 50) optimizationLevel = 'fair'
    else optimizationLevel = 'poor'

    // Generate recommendations
    if (lifestyle.sleep_quality && lifestyle.sleep_quality < 3) {
      recommendations.push('Focus on sleep hygiene optimization - consistent schedule, cool dark room, no screens before bed')
