  // 5. Physical Performance
  static calculatePhysicalPerformance(patientData: PatientData): HealthDomainResult {
    const { biomarkers, demographics, lifestyle } = patientData
    let score = 50
    const markersAvailable: string[] = []
    const markersMissing: string[] = ['Muscle Mass (DEXA)', 'Grip Strength', 'VO2 Max', 'Flexibility Tests', 'Balance Tests']
    const recommendations: string[] = []

    // Age-adjusted physical baseline
    if (demographics.age !== undefined) {
      markersAvailable.push('Age-Adjusted Assessment')
      // Baseline expectations by age group
      if (demographics.age <= 30) score += 10 // Peak physical potential
      else if (demographics.age >= 60) {
        score -= 10 // Natural age-related decline
        if (score >= 70) score += 10 // Bonus for exceptional aging
      }
    }

    // BMI and body composition
    if (demographics.bmi !== undefined) {
      markersAvailable.push('Body Composition (BMI)')
      if (demographics.bmi >= 18.5 && demographics.bmi <= 24.9) score += 15 // Optimal range
      else if (demographics.bmi >= 25 && demographics.bmi <= 29.9) score -= 5 // Overweight
      else if (demographics.bmi >= 30) score -= 20 // Obesity significantly impacts performance
      else score -= 10 // Underweight
    }

    // Physical activity level (direct performance indicator)
    if (lifestyle.physical_activity !== undefined) {
      markersAvailable.push('Physical Activity Level')
      if (lifestyle.physical_activity >= 6) score += 25 // Daily activity = excellent performance
      else if (lifestyle.physical_activity >= 4) score += 15 // Regular activity
      else if (lifestyle.physical_activity <= 2) score -= 25 // Sedentary = poor performance
      else score += 5 // Some activity
    }

    // Cardiovascular fitness markers
    if (patientData.systolic_bp !== undefined && patientData.diastolic_bp !== undefined) {
      markersAvailable.push('Cardiovascular Fitness')
      const systolic = patientData.systolic_bp
      if (systolic <= 120) score += 10 // Good cardiovascular fitness
      else if (systolic >= 140) score -= 15 // Poor cardiovascular fitness affects performance
    }

    // Metabolic fitness
    if (biomarkers.glucose !== undefined) {
      markersAvailable.push('Metabolic Fitness')
      if (biomarkers.glucose <= 90) score += 10 // Good metabolic fitness
      else if (biomarkers.glucose >= 110) score -= 10 // Poor glucose control affects performance
    }

    // Hemoglobin (oxygen transport capacity)
    if (biomarkers.hemoglobin !== undefined) {
      markersAvailable.push('Oxygen Transport Capacity')
      const isWoman = demographics.gender === 'female'
      const optimalHgb = isWoman ? 13.5 : 15.0
      const lowHgb = isWoman ? 11.0 : 12.0
      
      if (biomarkers.hemoglobin >= optimalHgb) score += 10 // Excellent oxygen transport
      else if (biomarkers.hemoglobin <= lowHgb) score -= 15 // Anemia impacts performance
    }

    // Recovery and inflammation
    if (biomarkers.c_reactive_protein !== undefined) {
      markersAvailable.push('Recovery Status (Inflammation)')
      if (biomarkers.c_reactive_protein <= 1.0) score += 8 // Good recovery
      else if (biomarkers.c_reactive_protein >= 3.0) score -= 12 // Poor recovery affects performance
    }

    // Sleep quality (recovery and performance)
    if (lifestyle.sleep_quality !== undefined) {
      markersAvailable.push('Sleep Quality (Recovery)')
      if (lifestyle.sleep_quality >= 4) score += 15 // Good sleep = better performance
      else if (lifestyle.sleep_quality <= 2) score -= 20 // Poor sleep = poor performance
    }

    // Smoking (oxygen delivery and endurance)
    if (lifestyle.smoking_status !== undefined) {
      markersAvailable.push('Smoking Status')
      if (lifestyle.smoking_status === 'never') score += 8
      else if (lifestyle.smoking_status === 'current') score -= 25 // Major performance inhibitor
      else score += 3 // Former smoker
    }

    const finalScore = Math.max(0, Math.min(100, score))
    
    let optimizationLevel: 'poor' | 'fair' | 'good' | 'excellent' | 'optimal'
    if (finalScore >= 85) optimizationLevel = 'optimal'
    else if (finalScore >= 75) optimizationLevel = 'excellent'
    else if (finalScore >= 65) optimizationLevel = 'good'
    else if (finalScore >= 50) optimizationLevel = 'fair'
    else optimizationLevel = 'poor'

    // Generate recommendations
    if (lifestyle.physical_activity && lifestyle.physical_activity < 4) {
      recommendations.push('Increase physical activity - combine cardiovascular and strength training')
    }
    if (demographics.bmi && demographics.bmi > 25) {
      recommendations.push('Optimize body composition through strength training and nutrition')
    }
    if (lifestyle.sleep_quality && lifestyle.sleep_quality < 3) {
      recommendations.push('Improve sleep quality for better recovery and performance')
    }
    if (lifestyle.smoking_status === 'current') {
      recommendations.push('Smoking cessation will dramatically improve physical performance and endurance')
    }
    if (recommendations.length === 0) {
      recommendations.push('Maintain excellent physical conditioning and consider performance optimization')
    }

    const confidence = markersAvailable.length >= 4 ? 'medium' : 'low' // Cannot be high without direct performance testing

    return {
      domain_name: 'Physical Performance',
      score_percentage: finalScore,
      confidence_level: confidence,
      markers_available: markersAvailable,
      markers_missing: markersMissing,
      optimization_level: optimizationLevel,
      recommendations: recommendations,
      description: `Physical performance score of ${finalScore}% based on age-adjusted fitness indicators, body composition, activity level, and physiological markers supporting physical function.`,
      algorithm_used: 'Age-adjusted physical performance assessment',
      reference: 'ACSM Position Stand on Exercise and Physical Activity 2018; WHO Physical Activity Guidelines 2020'
    }
  }

  // 6. Sleep Quality
  static calculateSleepQuality(patientData: PatientData): HealthDomainResult {
    const { biomarkers, demographics, lifestyle } = patientData
    let score = 50
    const markersAvailable: string[] = []
    const markersMissing: string[] = ['Sleep Study', 'HRV During Sleep', 'Melatonin Levels', 'Sleep Efficiency Tracking']
